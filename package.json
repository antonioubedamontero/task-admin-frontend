{
  "name": "task-admin-frontend",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start:dev": "ng serve --host 0.0.0.0 --port 4200",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "docker-build:prod": "docker build -t task-admin-frontend-prod -f Dockerfile.prod .",
    "docker-run-container:prod": "docker run -d -p 8080:80 task-admin-frontend-prod",
    "create-app:prod": "npm run docker-build:prod && npm run docker-run-container:prod",
    "docker-build:desa": "docker build -t task-admin-frontend-desa -f Dockerfile.dev .",
    "docker-run-container:desa": "docker run -d -p 4200:4200 -v .:/app task-admin-frontend-desa",
    "create-app:desa": "npm run docker-build:desa && npm run docker-run-container:desa",
    "test": "ng test --browsers=ChromeHeadless --watch=false",
    "test:coverage": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@tailwindcss/postcss": "^4.1.10",
    "lucide-angular": "^0.525.0",
    "moment": "^2.30.1",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.10",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.1.1",
    "eslint": "^9.29.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  }
}
